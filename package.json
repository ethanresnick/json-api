{
  "name": "json-api",
  "version": "3.0.0-alpha.8",
  "description": "A library for constructing JSON-API compliant responses",
  "homepage": "https://github.com/ethanresnick/json-api",
  "repository": {
    "type": "git",
    "url": "git://github.com/ethanresnick/json-api.git"
  },
  "author": {
    "name": "Ethan Resnick",
    "email": "ethan.resnick@gmail.com"
  },
  "license": "LGPL-3.0",
  "main": "build/src/index.js",
  "typings": "build/src/index.d.ts",
  "nyc": {
    "include": [
      "src/*.ts",
      "src/*.js",
      "src/**/*.ts",
      "src/**/*.js"
    ],
    "exclude": [
      "typings",
      "node_modules",
      "test",
      "templates",
      "build",
      "lib/index.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "@types/chai-subset": "^1.3.1",
    "@types/debug": "0.0.29",
    "@types/express": "^4.0.37",
    "@types/pluralize": "0.0.27",
    "@types/qs": "^6.5.0",
    "@types/ramda": "^0.24.9",
    "@types/url-template": "^2.0.28",
    "content-type": "1.x.x",
    "dasherize": "2.0.x",
    "debug": "^2.6.8",
    "flat": "^1.2.1",
    "immutable": "^3.7.5",
    "jade": "1.11.x",
    "lodash": "^3.10.0",
    "negotiator": "github:ethanresnick/negotiator#full-parse-access",
    "pluralize": "0.0.11",
    "qs": "^6.5.0",
    "ramda": "^0.24.1",
    "raw-body": "1.3.x",
    "url-template": "^2.0.4",
    "vary": "^1.1.1"
  },
  "devDependencies": {
    "@ethanresnick/tslint-config": "^5.5.1",
    "@types/mocha": "^2.2.42",
    "@types/mongodb": "^2.2.11",
    "@types/mongoose": "^4.7.21",
    "chai": "^1.9.2",
    "chai-subset": "^1.5.0",
    "coveralls": "^2.13.1",
    "cpr": "^2.2.0",
    "eslint": "0.x.x",
    "express": "^4.15.4",
    "gulp": "^3.8.6",
    "istanbul": "0.3.x",
    "mocha": "^2.5.3",
    "mongoose": "^4.7.9",
    "node-mongoose-fixtures": "^0.2.4",
    "nyc": "^11.1.0",
    "rimraf": "^2.6.1",
    "sinon": "1.10.2",
    "superagent": "1.2.x",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  },
  "peerDependencies": {
    "mongoose": "^4.7.0",
    "express": "^4.0.0"
  },
  "scripts": {
    "clean": "rimraf build",
    "lint": "tslint --project tsconfig.json --format verbose --config node_modules/@ethanresnick/tslint-config/tslint.json",
    "test": "export NODE_ENV=testing; mocha --compilers ts:ts-node/register --recursive test/unit/**/**/*.ts test/integration/**/**/*.ts",
    "build:lintfree": "npm run clean && cpr templates build/templates && echo Using TypeScript && tsc --version && tsc --pretty",
    "build": "npm run build:lintfree",
    "prepublish": "npm run test && npm run build:lintfree",
    "cover": "NODE_ENV=test nyc npm t",
    "cover:view": "npm run cover && open coverage/lcov-report/index.html",
    "watch": "npm run build -- --watch",
    "watch:lintfree": "npm run build:lintfree -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "bugs": {
    "url": "https://github.com/ethanresnick/json-api/issues"
  },
  "keywords": [
    "json-api",
    "jsonapi",
    "api",
    "hypermedia",
    "rest",
    "restful"
  ],
  "engines": {
    "node": ">=6.x.x"
  }
}
